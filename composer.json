{
  "type": "library",
  "name": "aeliot-tm/translation-maintain",
  "description": "The package that helps you keep your translations consistent",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Aeliot\\Bundle\\TransMaintain\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aeliot\\Bundle\\TransMaintain\\Test\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "google/cloud-translate": "^1.10",
    "symfony/config": ">=3.4",
    "symfony/console": ">=3.4",
    "symfony/dependency-injection": ">=3.4",
    "symfony/filesystem": ">=3.4",
    "symfony/finder": ">=3.4",
    "symfony/http-kernel": ">=3.4",
    "symfony/translation-contracts": "^v2.0.0",
    "symfony/yaml": ">=3.4"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "dg/bypass-finals": "^1.1",
    "friendsofphp/php-cs-fixer": "^3.8",
    "phpunit/phpunit": "^9.5",
    "symfony/dotenv": "^5.3",
    "symfony/framework-bundle": ">=3.4",
    "symfony/translation": "^5.3"
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit",
      "vendor/bin/php-cs-fixer fix -vv --using-cache=no"
    ]
  }
}
